@model Sistema_MVC_Web2.Models.EvidenciaCriterio
@{
    ViewBag.Title = "Visualizar";
}

<h2>Visualizar</h2>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="~/EvidenciaCriterio/Index">Evidencia Criterio</a>
        </li>
        <li class="breadcrumb-item" active>@Model.evidencia_id</li>

    </ol>

</nav>
<div class="card">
    <div class="card-header">
        Informacion detallada Evidencia Criterio

    </div>
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.evidencia_id</dd>
            <dt>Detalle Asignacion</dt>
            <dd>@Model.Criterio.nombre_english</dd>
            <dt>Docente</dt>
            <dd>@Model.archivo</dd>

            <dt>Tamaño</dt>
            <dd>@Model.tamanio</dd>

            <dt>Tipo</dt>
            <dd>@Model.tipo</dd>

            <dt>Descripcion</dt>
            <dd>@Model.descripcion</dd>

    

        </dl>

    </div>

</div>

