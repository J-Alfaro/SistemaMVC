@model Sistema_MVC_Web2.Models.DetalleAsignacion
@{
    ViewBag.Title = "Visualizar";
}

<h2>Visualizar</h2>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="~/DetalleAsignacion/Index">DetalleAsignacion</a>
        </li>
        <li class="breadcrumb-item" active>@Model.asignacion_id</li>

    </ol>

</nav>
<div class="card">
    <div class="card-header">
        Informacion detallada del detalleasignacion

    </div>
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.detalleasignacion_id</dd>
            <dt>Docente</dt>
            <dd>@Model.Docente.apellido , @Model.Docente.nombre</dd>
            <dt>DetalleAsignacion</dt>
            <dd>@Model.Asignacion.titulo</dd>
            <dt>Nivel</dt>
            <dd>@Model.Criterio.descripcion</dd>
            <dt>Estado</dt>
            <dd>@(Model.estado == "A" ? "Activo" : "Inactivo")</dd>

        </dl>

    </div>

</div>
